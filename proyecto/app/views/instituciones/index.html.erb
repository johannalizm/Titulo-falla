<p id="notice"><%= notice %></p>

<h1>Instituciones</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Pais</th>
      <th>Codigopostal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instituciones.each do |institucione| %>
      <tr>
        <td><%= institucione.nombre %></td>
        <td><%= institucione.pais %></td>
        <td><%= institucione.codigopostal %></td>
        <td><%= link_to 'Show', institucione %></td>
        <td><%= link_to 'Edit', edit_institucione_path(institucione) %></td>
        <td><%= link_to 'Destroy', institucione, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Institucione', new_institucione_path %>
